{
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/build": true
  },
  "files.eol": "\n",
  "typescript.preferences.importModuleSpecifier": "non-relative"
}
