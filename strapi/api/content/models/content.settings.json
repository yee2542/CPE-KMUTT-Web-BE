{
  "kind": "collectionType",
  "collectionName": "contents",
  "info": {
    "name": "Content",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "header": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "content_id": {
      "type": "uid"
    },
    "tags": {
      "type": "component",
      "repeatable": true,
      "component": "tags.tags",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "dynamic_sections": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "content-sections.carousal-image",
        "content-sections.content-component",
        "content-sections.grid-image"
      ],
      "required": true
    },
    "canvas_preview": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    }
  }
}
